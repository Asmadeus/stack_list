{
  "name": "fullstack-react-template",
  "version": "1.0.0",
  "description": "Starter template for React, React Router, Redux, Express with hot reload",
  "scripts": {
    "start:dev": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development node index.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "clean": "rimraf dist",
    "start": "cross-env NODE_ENV=production node index.js",
    "test": "jest",
    "test:watch": "jest --notify --watch"
  },
  "engines": {
    "node": "8.11.3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Asmadeus/fullstack-react-template.git"
  },
  "dependencies": {
    "express": "^4.16.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^23.5.0",
    "node-sass": "^4.9.0",
    "react-hot-loader": "^4.3.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-manifest-plugin": "^2.0.3",
    "webpack2-externals-plugin": "^1.0.0"
  }
}
